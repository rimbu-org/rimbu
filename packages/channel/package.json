{
  "name": "@rimbu/channel",
  "version": "3.0.3",
  "description": "Channel implementation for TypeScript akin to Go Channels",
  "keywords": [
    "channel",
    "rpc",
    "remote",
    "thread",
    "immutable",
    "rimbu",
    "typescript"
  ],
  "homepage": "https://rimbu.org",
  "author": {
    "name": "Arvid Nicolaas",
    "email": "arvid@rimbu.org",
    "url": "https://github.com/vitoke"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/vitoke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rimbu-org/rimbu.git",
    "directory": "packages/channel"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/entry/channel.d.mts",
      "import": "./dist/entry/channel.mjs"
    },
    "./*": {
      "types": "./dist/public/*.d.mts",
      "default": "./dist/public/*.mjs"
    }
  },
  "imports": {
    "#channel/*": {
      "types": "./dist/public/internal/*.d.mts",
      "default": "./dist/public/internal/*.mjs"
    },
    "#private/*": {
      "types": "./dist/private/*.d.mts",
      "default": "./dist/private/*.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun clean && bunx tsc --p tsconfig.esm.json && bunx tsc-alias -p tsconfig.esm.json",
    "clean": "rimraf dist",
    "extract-api": "yarn extract-api:main",
    "extract-api:main": "tsx ../../config/api-extractor.ts config/api-extractor.main.json",
    "format": "yarn format:base --write",
    "format:base": "prettier \"{!CHANGELOG.md}|**/**/*.{ts,tsx,js,mts,mjs,json,md}\"",
    "format:check": "yarn format:base --check",
    "lint": "eslint src",
    "test": "bun test --tsconfig-override tsconfig.common.json",
    "test:types": "bunx tsd --files ./test-d --typings ./dist/arr.d.mts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@rimbu/common": "workspace:*",
    "@rimbu/stream": "workspace:*",
    "tslib": "^2.8.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@types/node": "^20.11.6"
  }
}
