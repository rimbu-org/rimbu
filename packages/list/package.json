{
  "name": "@rimbu/list",
  "version": "2.1.10",
  "description": "An efficient immutable ordered sequence of elements akin to a Vector",
  "keywords": [
    "list",
    "vector",
    "random access",
    "sequence",
    "elements",
    "trie",
    "immutable",
    "collection",
    "rimbu",
    "typescript"
  ],
  "homepage": "https://rimbu.org",
  "author": {
    "name": "Arvid Nicolaas",
    "email": "arvid@rimbu.org",
    "url": "https://github.com/vitoke"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/vitoke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rimbu-org/rimbu.git",
    "directory": "packages/list"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/entry/list.d.mts",
      "default": "./dist/entry/list.mjs"
    },
    "./*": {
      "types": "./dist/public/*.d.mts",
      "default": "./dist/public/*.mjs"
    }
  },
  "imports": {
    "#list/*": {
      "types": "./dist/public/internal/*.d.mts",
      "default": "./dist/public/internal/*.mjs"
    },
    "#private/*": {
      "types": "./dist/private/*.d.mts",
      "default": "./dist/private/*.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun clean && bunx tsc --p tsconfig.esm.json",
    "clean": "rimraf dist",
    "extract-api": "yarn extract-api:main && yarn extract-api:custom",
    "extract-api:custom": "tsx ../../config/api-extractor.ts config/api-extractor.custom.json",
    "extract-api:main": "tsx ../../config/api-extractor.ts config/api-extractor.main.json",
    "format": "yarn format:base --write",
    "format:base": "prettier \"{!CHANGELOG.md}|**/**/*.{ts,tsx,js,mts,mjs,json,md}\"",
    "format:check": "yarn format:base --check",
    "lint": "eslint src",
    "test": "bun test test/* --tsconfig-override tsconfig.common.json",
    "test:random": "bun test test-random",
    "test:types": "bunx tsd --files ./test-d --typings ./dist/entry/list.d.mts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@rimbu/base": "workspace:*",
    "@rimbu/collection-types": "workspace:*",
    "@rimbu/common": "workspace:*",
    "@rimbu/stream": "workspace:*",
    "tslib": "^2.8.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
