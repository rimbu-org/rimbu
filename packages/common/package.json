{
  "name": "@rimbu/common",
  "version": "0.5.1",
  "description": "Common types and objects used in many other Rimbu packages",
  "keywords": [
    "common",
    "range",
    "reducer",
    "immutable",
    "rimbu",
    "typescript"
  ],
  "homepage": "http://rimbu.org",
  "author": {
    "name": "Arvid Nicolaas",
    "email": "arvid@rimbu.org",
    "url": "https://github.com/vitoke"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/vitoke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rimbu-org/rimbu.git",
    "directory": "packages/common"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.modules.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn bundle",
    "bundle": "microbundle",
    "clean": "rimraf dist",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "publish-package": "yarn publish",
    "test": "jest",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*": [
      "yarn format:single",
      "eslint"
    ]
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  }
}
