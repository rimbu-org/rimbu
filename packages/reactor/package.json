{
  "name": "@rimbu/reactor",
  "version": "0.14.10",
  "description": "React state management based on Rimbu Actors",
  "keywords": [
    "state management",
    "observable",
    "actor",
    "react",
    "rimbu",
    "typescript"
  ],
  "homepage": "https://rimbu.org",
  "author": {
    "name": "Arvid Nicolaas",
    "email": "arvid@rimbu.org",
    "url": "https://github.com/vitoke"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/vitoke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rimbu-org/rimbu.git",
    "directory": "packages/reactor"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.mts",
      "import": "./dist/esm/index.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && yarn bundle",
    "bundle": "tsc --p tsconfig.esm.json",
    "clean": "rimraf dist",
    "format": "yarn format:base --write",
    "format:base": "prettier \"{!CHANGELOG.md}|**/**/*.{ts,tsx,js,mts,mjs,json,md}\"",
    "format:check": "yarn format:base --check",
    "lint": "eslint src",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "happy-dom": "^9.20.3",
    "react": "<18.0.0",
    "react-dom": "<18.0.0"
  },
  "dependencies": {
    "@rimbu/actor": "^0.15.9",
    "@rimbu/deep": "^2.0.7",
    "@rimbu/stream": "^3.0.3",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
