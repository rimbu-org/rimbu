{
  "name": "@rimbu/deep",
  "version": "0.14.5",
  "description": "Tools to use handle plain JS objects as immutable objects",
  "keywords": [
    "immutable",
    "patch",
    "match",
    "plain object",
    "freeze",
    "frozen",
    "rimbu",
    "typescript"
  ],
  "homepage": "https://rimbu.org",
  "author": {
    "name": "Arvid Nicolaas",
    "email": "arvid@rimbu.org",
    "url": "https://github.com/vitoke"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/vitoke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rimbu-org/rimbu.git",
    "directory": "packages/deep"
  },
  "source": "./src/index.ts",
  "main": "./dist/main/index.js",
  "module": "./dist/module/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/module/index.js",
      "require": "./dist/main/index.js",
      "default": "./dist/module/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && yarn bundle",
    "build:deno": "rimraf deno_dist ../../deno_dist/deep && denoify && mv deno_dist ../../deno_dist/deep",
    "bundle": "yarn bundle:main && yarn bundle:module && yarn bundle:types",
    "bundle:main": "tsc --p tsconfig.main.json",
    "bundle:module": "tsc --p tsconfig.module.json",
    "bundle:types": "tsc --p tsconfig.types.json",
    "clean": "rimraf dist",
    "extract-api": "api-extractor run --local --verbose --config config/api-extractor.main.json",
    "format": "yarn format:base --write",
    "format:base": "prettier \"{!CHANGELOG.md}|**/**/*.{ts,tsx,js,json,md}\"",
    "format:check": "yarn format:base --check",
    "lint": "eslint src",
    "test": "jest",
    "test:types": "tsd",
    "typecheck": "tsc"
  },
  "sideEffects": false,
  "dependencies": {
    "@rimbu/base": "^0.11.4",
    "@rimbu/common": "*",
    "tslib": "^2.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
