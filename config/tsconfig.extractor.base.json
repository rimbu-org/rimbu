{
  "compilerOptions": {
    // targets
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node16",
    // common
    "lib": ["esnext"],
    "importHelpers": true,
    "sourceMap": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "isolatedModules": true,
    "paths": {
      "@rimbu/actor": ["../packages/actor/dist/types"],
      "@rimbu/actor/immer": ["../packages/actor/dist/types/immer"],
      "@rimbu/actor/patch": ["../packages/actor/dist/types/patch"],
      "@rimbu/actor/stream": ["../packages/actor/dist/types/stream"],
      "@rimbu/base": ["../packages/base/dist/types"],
      "@rimbu/bimap": ["../packages/bimap/dist/types/main"],
      "@rimbu/bimap/custom": ["../packages/bimap/dist/types/custom"],
      "@rimbu/bimultimap": ["../packages/bimultimap/dist/types/main"],
      "@rimbu/bimultimap/custom": ["../packages/bimultimap/dist/types/custom"],
      "@rimbu/collection-types": [
        "../packages/collection-types/dist/types/main"
      ],
      "@rimbu/collection-types/map": [
        "../packages/collection-types/dist/types/map"
      ],
      "@rimbu/collection-types/map-custom": [
        "../packages/collection-types/dist/types/map-custom"
      ],
      "@rimbu/collection-types/set": [
        "../packages/collection-types/dist/types/set"
      ],
      "@rimbu/collection-types/set-custom": [
        "../packages/collection-types/dist/types/set-custom"
      ],
      "@rimbu/common": ["../common/dist/types"],
      "@rimbu/core": ["../packages/core/dist/types/main"],
      "@rimbu/core/menu": ["../packages/core/dist/types/menu"],
      "@rimbu/deep": ["../packages/deep/dist/types"],
      "@rimbu/graph": ["../packages/graph/dist/types/main"],
      "@rimbu/graph/custom": ["../packages/graph/dist/types/custom"],
      "@rimbu/hashed": ["../packages/hashed/dist/types/main"],
      "@rimbu/hashed/map": ["../packages/hashed/dist/types/map"],
      "@rimbu/hashed/map-custom": ["../packages/hashed/dist/types/map-custom"],
      "@rimbu/hashed/set": ["../packages/hashed/dist/types/set"],
      "@rimbu/hashed/set-custom": ["../packages/hashed/dist/types/set-custom"],
      "@rimbu/list": ["../packages/list/dist/types/main"],
      "@rimbu/list/custom": ["../packages/list/dist/types/custom"],
      "@rimbu/multimap": ["../packages/multimap/dist/types/main"],
      "@rimbu/multimap/custom": ["../packages/multimap/dist/types/custom"],
      "@rimbu/multiset": ["../packages/multiset/dist/types/main"],
      "@rimbu/multiset/custom": ["../packages/multiset/dist/types/custom"],
      "@rimbu/ordered": ["../packages/ordered/dist/types/main"],
      "@rimbu/ordered/map": ["../packages/ordered/dist/types/map"],
      "@rimbu/ordered/map-custom": [
        "../packages/ordered/dist/types/map-custom"
      ],
      "@rimbu/ordered/set": ["../packages/ordered/dist/types/set"],
      "@rimbu/ordered/set-custom": [
        "../packages/ordered/dist/types/set-custom"
      ],
      "@rimbu/reactor": ["../packages/reactor/dist/types"],
      "@rimbu/sorted": ["../packages/sorted/dist/types/main"],
      "@rimbu/sorted/common": ["../packages/sorted/dist/types/common"],
      "@rimbu/sorted/map": ["../packages/sorted/dist/types/map"],
      "@rimbu/sorted/map-custom": ["../packages/sorted/dist/types/map-custom"],
      "@rimbu/sorted/set": ["../packages/sorted/dist/types/set"],
      "@rimbu/sorted/set-custom": ["../packages/sorted/dist/types/set-custom"],
      "@rimbu/spy": ["../packages/spy/dist/types"],
      "@rimbu/stream": ["../packages/stream/dist/types/main"],
      "@rimbu/stream/async": ["../packages/stream/dist/types/async"],
      "@rimbu/stream/async-custom": [
        "../packages/stream/dist/types/async-custom"
      ],
      "@rimbu/stream/custom": ["../packages/stream/dist/types/custom"],
      "@rimbu/table": ["../packages/table/dist/types/main"],
      "@rimbu/table/custom": ["../packages/table/dist/types/custom"],
      "@rimbu/table/hash-row": ["../packages/table/dist/types/hash-row"],
      "@rimbu/table/sorted-row": ["../packages/table/dist/types/sorted-row"],
      "@rimbu/typical": ["../packages/typical/dist/types"]
    }
  }
}
